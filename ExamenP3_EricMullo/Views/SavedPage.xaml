<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            x:Class="ExamenTercerProgreso_Eric.Views.SavedPage">
    <StackLayout>
        <ListView x:Name="scordova_listView"
                 ItemsSource="{Binding Aeropuertos}">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding Nombre}" Detail="{Binding Pais}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>